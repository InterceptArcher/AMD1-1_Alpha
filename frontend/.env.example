# Backend API URL
# This value must be provided via environment variables
NEXT_PUBLIC_API_URL=http://localhost:8000

# For production deployment, set this to your Railway backend URL
# Example: NEXT_PUBLIC_API_URL=https://your-backend.railway.app

# Supabase Configuration
# These values must be provided via environment variables
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Server-side only - never expose to client
SUPABASE_SERVICE_ROLE=your-service-role-key
